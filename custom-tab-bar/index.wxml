<!--custom-tab-bar/index.wxml-->
<i-tab-bar current="{{ current }}" color="#1b9aee" bindchange="handleChange">
    <i-tab-bar-item key="home" icon="homepage" current-icon="homepage_fill" title="首页"></i-tab-bar-item>
    <i-tab-bar-item key="fire" icon="activity" current-icon="activity_fill" title="办税"></i-tab-bar-item>
    <i-tab-bar-item key="index" icon="remind" current-icon="remind_fill" title="服务"></i-tab-bar-item>
    <i-tab-bar-item key="my" icon="mine" current-icon="mine_fill" dot title="个人中心"></i-tab-bar-item>
  </i-tab-bar>
  
  <view class="is-ios"></view>